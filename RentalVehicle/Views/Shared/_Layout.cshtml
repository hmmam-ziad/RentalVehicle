            <div class="container">
                <div class="row gx-0 align-items-center" style="height: 45px;">
                    <div class="col-lg-6 text-center text-lg-start mb-lg-0">
                        <div class="d-flex flex-wrap">
                            <a asp-controller="Home" asp-action="Contact" asp-route-fragment="loca" class="text-muted me-4"><i class="fas fa-map-marker-alt text-primary me-2"></i>Find A Location</a>
                            <a href="tel:+962782404091" class="text-muted me-4"><i class="fas fa-phone-alt text-primary me-2"></i>+962 7 8240 4091</a>
                            <a href="mailto:hmmamxxc@gmail.com" class="text-muted me-0"><i class="fas fa-envelope text-primary me-2"></i>hmmamxxc@gmail.com</a>
                        </div>
                    </div>
                    <div class="col-lg-6 text-center text-lg-end">
                        <div class="d-flex align-items-center justify-content-end">
                            <a href="https://www.facebook.com/0o0o0o0o0o0o0o0o0o0o0o0o.hmmam?mibextid=ZbWKwL" class="btn btn-light btn-sm-square rounded-circle me-3"><i class="fab fa-facebook-f"></i></a>
                            <a href="#" class="btn btn-light btn-sm-square rounded-circle me-3"><i class="fab fa-twitter"></i></a>
                            <a href="https://www.instagram.com/4._3c?igsh=Nmt5djQwd3VkajNm" class="btn btn-light btn-sm-square rounded-circle me-3"><i class="fab fa-instagram"></i></a>
                            <a href="https://www.linkedin.com/in/hmmam-alsokney-9b0044300?utm_source=share&utm_campaign=share_via&utm_content=profile&utm_medium=android_app" class="btn btn-light btn-sm-square rounded-circle me-0"><i class="fab fa-linkedin-in"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Topbar End -->
        <!-- Navbar & Hero Start -->
        <div class="container-fluid nav-bar sticky-top px-0 px-lg-4 py-2 py-lg-0">
            <div class="container">
                <nav class="navbar navbar-expand-lg navbar-light">
                    <a href="" class="navbar-brand p-0">
                        <h1 class="display-6 text-primary"><i class="fas fa-car-alt me-3"></i>Cental</h1>
                        <!-- <img src="img/logo.png" alt="Logo"> -->
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                        <span class="fa fa-bars"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarCollapse">
                        <div class="navbar-nav mx-auto py-0">
                            <a asp-controller="Home" asp-action="Index" class="nav-item nav-link active">Home</a>
                            <a asp-controller="Home" asp-action="Privacy" class="nav-item nav-link">Privacy</a>
                            @* <a href="about.html" class="nav-item nav-link">About</a> *@
                            <a asp-controller="Vehicles" asp-action="Index" class="nav-item nav-link">Our Cars</a>
                            <a asp-controller="Home" asp-action="Service" class="nav-item nav-link">Service</a>
                            @if (HttpContextAccessor.HttpContext.Session.GetString("isLogIn") == "true")
                            {
                                @if (HttpContextAccessor.HttpContext.Session.GetString("Role") == "Admin"){
                                    <div class="nav-item dropdown">
                                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Content Managment</a>
                                        <div class="dropdown-menu m-0">
                                            <a asp-controller="VehicleReturns" asp-action="Index" class="dropdown-item">Vehicle Returns</a>
                                            <a asp-controller="VehicleCategories" asp-action="Index" class="dropdown-item">Vehicle Categories</a>
                                            <a asp-controller="RentalTransactions" asp-action="Index" class="dropdown-item">Rental Transactions</a>
                                            <a asp-controller="RentalBookings" asp-action="Index" class="dropdown-item">Rental Bookings</a>
                                            <a asp-controller="Maintenances" asp-action="Index" class="dropdown-item">Maintenances</a>
                                            <a asp-controller="FuleTypes" asp-action="Index" class="dropdown-item">Fule Types</a>
                                            <a asp-controller="Customer" asp-action="Index" class="dropdown-item">Customer</a>
                                        </div>
                                    </div>
                                    <a asp-controller="RentalBookings" asp-action="Index" class="nav-item nav-link">Bookings For All Customers</a>
                                    
                                }
                                else {
                                    <a asp-controller="RentalBookings" asp-action="Index" class="nav-item nav-link">Your Booking Details</a>
                                }
                                <a asp-controller="Customer" asp-action="Edit" asp-route-id="@HttpContextAccessor.HttpContext.Session.GetInt32("CustomerID")" class="nav-item nav-link">Profile</a>
                                
                            }
                            <a asp-controller="Home" asp-action="Contact" class="nav-item nav-link">Contact</a>
                            
                        </div>
                        @if (HttpContextAccessor.HttpContext.Session.GetString("isLogIn") == "true")
                        {
                            <a asp-controller="Customer" asp-action="Logout" class="btn btn-primary rounded-pill py-2 px-4">Logout</a>
                        }
                        else {
                            <a asp-controller="Customer" asp-action="Login" class="btn btn-primary rounded-pill py-2 px-4">Login</a>
                        }                       
                    </div>
                </nav>
            </div>
        </div>
        <!-- Navbar & Hero End -->
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- Footer Start -->
    <div class="container-fluid footer py-5 wow fadeIn" data-wow-delay="0.2s">
        <div class="container py-5">
            <div class="row g-5">
                <div class="col-md-6 col-lg-6 col-xl-3">
                    <div class="footer-item d-flex flex-column">
                        <div class="footer-item">
                            <h4 class="text-white mb-4">About Us</h4>
                            <p class="mb-3" id="moree">We are dedicated to providing exceptional experiences, focusing on quality and customer satisfaction. Our team is committed to excellence, ensuring that every interaction reflects our passion for what we do.</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-6 col-xl-3">
                    <div class="footer-item d-flex flex-column">
                        <h4 class="text-white mb-4">Quick Links</h4>
                        <a asp-controller="Vehicles" asp-action="Index"><i class="fas fa-angle-right me-2"></i> Cars</a>
                        <a href="#"><i class="fas fa-angle-right me-2"></i>Hmmam zaid(Leader)</a>
                        <a href="#"><i class="fas fa-angle-right me-2"></i>Abdalrhman Bassam</a>
                        <a href="#"><i class="fas fa-angle-right me-2"></i>Shahed</a>
                        <a href="#"><i class="fas fa-angle-right me-2"></i>Demaa</a>
                        <a href="#"><i class="fas fa-angle-right me-2"></i>Naqaa</a>
